<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="match_parent"
    tools:context=".pantai">

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="587dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="326dp"
        tools:layout_editor_absoluteY="0dp">


        <TextView
            android:id="@+id/textView4"
            android:layout_width="fill_parent"
            android:layout_height="42dp"
            android:text="Wisata Pantai"
            android:textAlignment="center"
            android:textSize="30sp"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp" />

        <Button
            android:id="@+id/buttonTanjungBaru"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="Pantai Tanjung Baru"
            tools:layout_editor_absoluteX="144dp"
            tools:layout_editor_absoluteY="70dp" />

        <Button
            android:id="@+id/buttonTanjungPakis"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="Pantai Tanjung Pakis"
            tools:layout_editor_absoluteX="131dp"
            tools:layout_editor_absoluteY="151dp" />

        <Button
            android:id="@+id/buttonSamuderaBaru"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="Samudera Baru"
            tools:layout_editor_absoluteX="37dp"
            tools:layout_editor_absoluteY="225dp" />
    </LinearLayout>
</android.support.constraint.ConstraintLayout>